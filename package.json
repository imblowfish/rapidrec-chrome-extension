{
  "name": "rapidrec-chrome-extension",
  "description": "Instant screen recording browser extension with floating camera view",
  "version": "0.3.0",
  "scripts": {
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:storybook": "jest --silent --rootDir='./src/components' --json --outputFile='.jest-test-results.json'",
    "build": "webpack --progress --config webpack.prod.cjs",
    "dev": "webpack --progress --watch --config webpack.dev.cjs",
    "clean": "rm -rf ./ext/*.mjs* ./coverage .jest-test-results.json",
    "lint": "eslint --ext .mjs --ext .ts --ext .tsx .",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "storybook": "npm run test:storybook && start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imblowfish/rapidrec-chrome-extension"
  },
  "author": {
    "name": "Yury Getman",
    "email": "imblowfish@duck.com",
    "url": "https://github.com/imblowfish"
  },
  "bugs": {
    "url": "https://github.com/imblowfish/rapidrec-chrome-extension/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "babel-loader": "^9.1.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@mdx-js/react": "^2.2.1",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-jest": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack4": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack4": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@types/chrome": "^0.0.208",
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint-plugin-storybook": "^0.6.10",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "storybook": "^6.5.15",
    "ts-jest": "^29.0.5"
  }
}
