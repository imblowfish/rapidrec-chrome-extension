{
  "name": "Rapidrec",
  "description": "Write me",
  "version": "0.2.2",
  "manifest_version": 3,
  "action": {
    "default_popup": "./popup.html"
  },
  "background": {
    "service_worker": "./background.bundle.mjs",
    "type": "module"
  },
  "permissions": [
    "scripting",
    "tabs",
    "downloads",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
